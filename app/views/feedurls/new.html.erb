
<h1>New Feed Url</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@feedurl) do |f| %>
       <%= render 'layouts/error_messages', object: f.object %>
      <%= f.label :feed_url %>
      <%= f.text_field :feed_url %>

        
	  <br>
      <%= f.submit "Submit", class: "btn btn-large btn-primary" %>
     
    <% end %>
     <%= link_to 'Back', feedurls_path %>
  </div>
</div>

