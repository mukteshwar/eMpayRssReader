
<h1>Update Feed Url</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@feedurl) do |f| %>
     
      <%= f.label :feed_url %>
      <%= f.text_field :feed_url %>

       <%= f.submit "Update", class: "btn btn-large btn-primary" %>
    <% end %>
	<%= link_to 'Back', feedurls_path %>
  </div>
</div>

